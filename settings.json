{
    "workbench.iconTheme": "icons",
    "editor.tabSize": 2,
    
    "editor.fontFamily": "Fira Code Retina",
    "editor.fontSize": 14,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Bearded Theme feat. Will",
    "github.copilot.editor.enableAutoCompletions": true,

    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],

    "vim.handleKeys": {
    "<C-p>": false,
    "<C-w>": false,
    "<C-b>": false,
    "<C-f": false
    }
}